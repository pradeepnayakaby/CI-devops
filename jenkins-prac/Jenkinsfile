pipeline{
    agent any
    stages{
        stage("git check out"){
            steps{
                git branch: 'jenkins ', url: 'https://github.com/pradeepnayakaby/CI-devops.git'
            }
        }
        stage("build"){
            steps{
                sh 'chmod 777 shell.sh'
                sh './shell.sh'
            }
        }
    }
}